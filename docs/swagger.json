        "/updateByCSV/": {
            "post": {
                "description": "The request will update the database creating degrees, subjects, years, groups and hours",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "type": "file",
                        "description": "csv file",
                        "name": "csv",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
